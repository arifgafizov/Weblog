<h2 class="text-center"><em>Latest posts:</em></h2>
<br/>
<% @results.each do |post| %>
	<p>
		<br/>
		<p class="text-center"><%= post['content'] %></p>
		<br/>
		<span style="font-size: 100%; color: #428bca"><p class="text-center"><%= post['username'] %></p></span>
		<br/>
		<span style="font-size: 80%; color: #999%"><p class="text-center">Added at <%= post['created_date'] %></p></span>

	</p>
<% end %>